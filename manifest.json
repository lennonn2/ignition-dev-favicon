{
  "name": "Ignition Dev Favicon",
  "description": "Change favicon in dev mode",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["*://127.0.0.1/*"],
      "js": ["popup.js"]
    }
  ],
  "web_accessible_resources": ["favicon-purple.ico", "favicon-green.ico"],
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}